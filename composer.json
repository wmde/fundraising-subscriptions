{
	"name": "wmde/fundraising-subscriptions",
	"license": "GPL-2.0-or-later",
	"description": "Bounded Context for fundraising subcriptions",
	"require": {
		"php": ">=7.2",

		"doctrine/orm": "~2.5",
		"psr/log": "~1.0",

		"wmde/fundraising-store": "~10.1",
		"wmde/email-address": "~1.0",
		"wmde/fun-validators": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.1",
		"ockcyp/covers-validator": "~0.7.1",
		"squizlabs/php_codesniffer": "~3.2",
		"slevomat/coding-standard": "~4.5",
		"mediawiki/mediawiki-codesniffer": "~18.0",
		"phpstan/phpstan": "~0.9.2"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/wmde/fundraisingstore",
			"no-api": true
		}
	],
	"autoload": {
		"psr-4": {
			"WMDE\\Fundraising\\SubscriptionContext\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WMDE\\Fundraising\\SubscriptionContext\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
